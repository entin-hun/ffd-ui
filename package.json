    "@studiometa/vue-mapbox-gl": "^2.2.0",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.2.1",
    "chart.js": "^4.4.0",
    "dotenv": "^16.3.1",
    "luxon": "^3.4.3",
    "mapbox-gl": "^2.15.0",
    "quasar": "^2.6.0",
    "query-string": "^8.1.0",
    "vue": "^3.0.0",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.0.0"
  },
